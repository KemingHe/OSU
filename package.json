{
  "name": "@keminghe/osu",
  "version": "0.2.0",
  "description": "Unofficial, publicly available data about The Ohio State University.",
  "keywords": [],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KemingHe/OSU.git"
  },
  "homepage": "https://github.com/KemingHe/OSU",
  "bugs": {
    "url": "https://github.com/KemingHe/OSU/issues",
    "email": "keminghe.career@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Keming He",
    "email": "keminghe.career@gmail.com",
    "url": "https://linkedin.com/in/keminghe"
  },
  "contributors": [],
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "echo 'Currently using pnpm@9.6.0, remember to update: [ package.json and .github/workflows ].'",
    "lint": "biome ci .",
    "lint:fix": "biome check --write .",
    "test": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest --coverage.enabled --watch=false",
    "test:bench": "NODE_ENV=test vitest bench --outputFile.json=benchmark.json",
    "test:bench:compare": "NODE_ENV=test vitest bench --compare benchmark.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "set -e; tsc --project tsconfig.esm.json; tsc-alias -p tsconfig.esm.json;",
    "prebuild": "tsx scripts/generate.ts",
    "build": "set -e; rm -rf ./dist; pnpm run build:cjs; pnpm run build:esm;",
    "postbuild": "echo 'Build complete: src/ -> dist/cjs, dist/esm, and dist/types.'"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^20.14.13",
    "@vitest/coverage-v8": "^2.0.5",
    "commitizen": "^4.3.0",
    "csv-parser": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "mock-fs": "^5.2.0",
    "release-it": "^17.6.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.3",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
